pipeline {
       agent any
    stages {
	      stage('build') {
	          step {
		    echo "building the code..."
		    }
		  }
	      stage('test') {
	       step {
		  echo "testing the code"
		  }
		  }
	      stage('deploy') {
	       step {
		  echo "deploying the code"
		  }
		  }
	      stage('release') {
	       step {
		  echo "deploying the code"
		  }
		  }
    }
}
